{
  "name": "@dewfall/vhooks",
  "version": "0.1.1",
  "description": "vue3 hooks like ahooks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/dewfall123/vhooks.git",
  "author": "dewfall",
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "vlib-docsify --docRoot=docs",
    "deploy": "gh-pages -d docs",
    "deploy-view": "http-server ./docs",
    "build": "lerna run build",
    "test": "lerna run test",
    "pub": "lerna publish",
    "lint": "eslint --ignore-pattern **/__tests__/* --ignore-pattern **/dist/* \"packages/*/src/**/*.{vue,js,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "vue",
    "hooks",
    "ahooks"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "homepage": "https://dewfall123.github.io/vhooks/",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.159",
    "@types/lodash-es": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "del": "^5.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.1",
    "gh-pages": "^3.1.0",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "jest": "^25.1.0",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.26.4",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "vitepress": "^0.5.0",
    "vlib-docsify": "^0.2.0",
    "vue": "^3.0.1",
    "vue-router": "^4.0.0-beta.7"
  },
  "dependencies": {
    "dayjs": "^1.8.34",
    "intersection-observer": "^0.11.0",
    "localforage": "^1.9.0",
    "lodash-es": "^4.17.15",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "^5.0.2",
    "vue-demi": "^0.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
