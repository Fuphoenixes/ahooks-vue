{
  "name": "ahooks-vue",
  "version": "0.8.1",
  "description": "ahooks vue",
  "main": "packages/vhooks/dist/index.js",
  "types": "packages/vhooks/dist/index.d.ts",
  "repository": "https://github.com/dewfall123/vhooks.git",
  "author": "dewfall",
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "vitepress-fc --root=docs",
    "test": "jest",
    "build": "lerna run build",
    "docs-deploy": "gh-pages -d docs/dist",
    "docs-build-deploy": "yarn docs-build && yarn docs-deploy",
    "docs-build": "rimraf docs/dist && cross-env NODE_ENV=production vitepress-fc build --root=docs",
    "docs-static": "node static-serve.js",
    "pub": " yarn build && lerna publish",
    "lint": "eslint --ignore-pattern **/__tests__/* --ignore-pattern **/dist/* \"packages/*/src/**/*.{vue,js,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "keywords": [
    "ahooks-vue",
    "ahooks",
    "vue",
    "vue3",
    "hooks"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "homepage": "https://dewfall123.github.io/vhooks/",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.159",
    "@types/lodash-es": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "axios": "^0.21.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "koa": "^2.13.0",
    "koa-static-server": "^1.5.2",
    "lerna": "^3.22.1",
    "lodash": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.26.4",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "vitepress-for-component": "latest",
    "vue": "^3.0.2",
    "vue-router": "^4.0.0-beta.7",
    "whatwg-fetch": "^3.5.0"
  },
  "dependencies": {
    "dayjs": "^1.8.34",
    "intersection-observer": "^0.11.0",
    "localforage": "^1.9.0",
    "lodash-es": "^4.17.15",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "^5.0.2",
    "vue-demi": "^0.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
